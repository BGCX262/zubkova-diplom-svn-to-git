@model DogsClub.WebUI.Models.Confern
<div id="home">
    <div class="overlay">
        <div class="container">
            <div class="row ">
                <div class="col-md-4 col-md-offset-4">
                    <div class="div-trans text-center Authorization logout">
                        <h3 class="HeadRegistration">Хотите выйти ?</h3>
                        @using (Html.BeginForm())
                        {
                            <button type="submit" name="Confirm" value="true" class="btn btn-success btn-block btn-lg">Да</button>
                            <button type="submit" name="Confirm" value="false" class="btn btn-success btn-block btn-lg">Нет</button>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>